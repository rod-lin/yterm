{
  "name": "yterm",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "server": "npx ts-node src/server/main.ts",
    "babel": "npx babel src/yterm --out-dir dist/yterm --extensions .ts --source-maps inline",
    "build": "npx tsc && npm run babel && npx babel src/demo.ts --out-file dist/demo.js --extensions .ts --source-maps inline && npx browserify dist/demo.js -o dist/demo.js",
    "test": "npx tsc && npx mocha -r ts-node/register tests/**/*.test.ts",
    "coverage": "npx nyc -r text -r lcov -n src/yterm -e .ts npm run test"
  },
  "author": "zl38",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.5",
    "@types/ws": "^6.0.3",
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "node-pty": "^0.9.0",
    "ws": "^7.2.0"
  }
